{
    "description": "SKLINET Next.js Strapi boilerplate",
    "repository": {
        "type": "git",
        "url": "https://github.com/SKLINET/strapi-boilerplate"
    },
    "license": "MIT",
    "scripts": {
        "build": "env-cmd --silent graphql-codegen  --config codegen.yml && npm run relay && NODE_ENV=production next build",
        "build-storybook": "build-storybook -c .storybook -o ./public/storybook --quiet",
        "dev": "next dev",
        "generate-models": "node ./scripts/generateModels.js",
        "export": "NODE_ENV=production next export",
        "create-blocks": " node ./scripts/createBlocks.js && npm run relay",
        "graphql-codegen": "env-cmd -f .env graphql-codegen --config codegen.yml",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "relay": "node_modules/relay-compiler/cli.js",
        "relay:watch": "node_modules/relay-compiler/cli.js --watch",
        "start": "NODE_ENV=production next start",
        "storybook": "start-storybook --no-dll"
    },
    "engines": {
        "node": ">=16.13.0 <=18.x.x"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.0.1",
        "@react-google-maps/api": "^2.18.1",
        "@symbio/cms": "^1.0.0-canary.60",
        "@symbio/headless": "canary",
        "axios": "^1.3.5",
        "busboy": "^1.6.0",
        "clsx": "^1.2.1",
        "dayjs": "^1.11.7",
        "dotenv-webpack": "^8.0.1",
        "graphql": "^16.6.0",
        "graphql-tag": "^2.12.6",
        "hls.js": "^1.1.5",
        "html-react-parser": "^3.0.15",
        "http-proxy": "^1.18.1",
        "iron-session": "^6.3.1",
        "mandrill-api": "^1.0.45",
        "next": "^13.3.0",
        "nextjs-progressbar": "0.0.16",
        "nodemailer": "^6.7.5",
        "react": "^18.2.0",
        "react-18-image-lightbox": "^5.1.4",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.43.9",
        "react-relay": "^15.0.0",
        "react-responsive-carousel": "^3.2.23",
        "relay-runtime": "^15.0.0",
        "storybook-addon-swc": "^1.1.9",
        "swr": "^2.1.3",
        "uuid": "^9.0.0",
        "yup": "^1.1.1"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^3.3.0",
        "@graphql-codegen/introspection": "3.0.1",
        "@graphql-codegen/schema-ast": "^3.0.1",
        "@next/bundle-analyzer": "^13.3.0",
        "@storybook/addon-a11y": "^7.0.5",
        "@storybook/addon-actions": "^7.0.5",
        "@storybook/addon-backgrounds": "^7.0.5",
        "@storybook/addon-console": "^1.2.3",
        "@storybook/addon-controls": "^7.0.5",
        "@storybook/addon-docs": "^7.0.5",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/addon-storysource": "^7.0.5",
        "@storybook/addon-viewport": "^7.0.5",
        "@storybook/addons": "^7.0.5",
        "@storybook/builder-webpack5": "^7.0.5",
        "@storybook/client-api": "^7.0.5",
        "@storybook/manager-webpack5": "^6.5.16",
        "@storybook/react": "^7.0.5",
        "@svgr/webpack": "^7.0.0",
        "@types/busboy": "^1.5.0",
        "@types/http-proxy": "^1.17.9",
        "@types/mandrill-api": "^1.0.30",
        "@types/node": "^18.15.11",
        "@types/nodemailer": "^6.4.4",
        "@types/react": "^18.0.35",
        "@types/react-dom": "^18.0.11",
        "@types/react-relay": "^14.1.3",
        "@types/relay-runtime": "^14.1.10",
        "@types/uuid": "^9.0.1",
        "@types/yup": "^0.32.0",
        "@typescript-eslint/eslint-plugin": "^5.58.0",
        "@typescript-eslint/parser": "^5.58.0",
        "@welldone-software/why-did-you-render": "^7.0.1",
        "autoprefixer": "^10.4.14",
        "dotenv": "^16.0.3",
        "env-cmd": "^10.1.0",
        "eslint": "^8.38.0",
        "eslint-config-next": "^13.3.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "lint-staged": "^13.2.1",
        "postcss": "^8.4.21",
        "prettier": "^2.8.7",
        "relay-compiler": "^15.0.0",
        "relay-compiler-language-typescript": "^15.0.1",
        "relay-config": "^12.0.1",
        "sass": "^1.62.0",
        "sass-loader": "^13.2.2",
        "tailwindcss": "^3.3.1",
        "typescript": "^5.0.4"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "prettier": {
        "semi": true,
        "trailingComma": "all",
        "printWidth": 120,
        "tabWidth": 4,
        "singleQuote": true
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "es2021": true,
            "node": true
        },
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "ecmaFeatures": {
                "jsx": true
            },
            "ecmaVersion": 12,
            "sourceType": "module"
        },
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended",
            "next",
            "next/core-web-vitals",
            "plugin:prettier/recommended"
        ]
    }
}

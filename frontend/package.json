{
    "description": "SKLINET Next.js Strapi boilerplate",
    "repository": {
        "type": "git",
        "url": "https://github.com/SKLINET/strapi-boilerplate"
    },
    "license": "MIT",
    "scripts": {
        "build": "env-cmd --silent graphql-codegen --config codegen.yml && npm run relay && NODE_ENV=production next build",
        "build:only": "NODE_ENV=production next build",
        "dev": "next dev --turbopack",
        "generate-models": "node ./scripts/generateModels.js",
        "create-blocks": "node ./scripts/createBlocks.js && npm run relay",
        "graphql-codegen": "env-cmd -f .env graphql-codegen --config codegen.yml",
        "lint": "tsc --noEmit && eslint . --fix",
        "relay": "./node_modules/relay-compiler/cli.js",
        "relay:watch": "./node_modules/relay-compiler/cli.js --watch",
        "start": "NODE_ENV=production next start"
    },
    "engines": {
        "node": ">=20.9.0",
        "npm": ">=6.0.0"
    },
    "dependencies": {
        "@cloudinary/url-gen": "^1.21.0",
        "@elastic/elasticsearch": "^8.10.0",
        "@formatjs/intl-localematcher": "^0.5.2",
        "@hookform/resolvers": "^3.3.2",
        "@next/third-parties": "^16.1.4",
        "@react-google-maps/api": "^2.19.2",
        "axios": "^1.6.2",
        "busboy": "^1.6.0",
        "clsx": "^2.0.0",
        "dayjs": "^1.11.10",
        "graphql": "^16.8.1",
        "html-react-parser": "^5.0.6",
        "http-proxy": "^1.18.1",
        "iron-session": "^8.0.1",
        "negotiator": "^0.6.3",
        "next": "^16.1.4",
        "nextjs-toploader": "^1.6.12",
        "nodemailer": "^6.7.5",
        "nprogress": "^0.2.0",
        "react": "^19.2.3",
        "react-countup": "^6.5.3",
        "react-dom": "^19.2.3",
        "react-hook-form": "^7.48.2",
        "react-relay": "^20.1.1",
        "react-remove-scroll": "^2.6.0",
        "react-responsive-carousel": "^3.2.23",
        "react-scroll-parallax": "^3.4.5",
        "react-share": "^5.0.3",
        "relay-runtime": "^20.1.1",
        "swr": "^2.2.4",
        "uuid": "^9.0.1",
        "yup": "^1.3.2"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.0.0",
        "@eslint/js": "^9.0.0",
        "@graphql-codegen/cli": "^5.0.0",
        "@graphql-codegen/introspection": "4.0.0",
        "@graphql-codegen/schema-ast": "^4.0.0",
        "@next/bundle-analyzer": "^16.1.4",
        "@svgr/webpack": "^8.1.0",
        "@types/busboy": "^1.5.3",
        "@types/http-proxy": "^1.17.9",
        "@types/negotiator": "^0.6.3",
        "@types/node": "^20.9.4",
        "@types/nodemailer": "^6.4.4",
        "@types/nprogress": "^0.2.3",
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "@types/relay-runtime": "^20.1.1",
        "@types/uuid": "^9.0.7",
        "dotenv": "^16.3.1",
        "env-cmd": "^10.1.0",
        "eslint": "^9.0.0",
        "eslint-config-next": "^16.1.4",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "glob": "^10.0.0",
        "lint-staged": "^15.1.0",
        "postcss": "^8.4.31",
        "prettier": "^3.1.0",
        "relay-compiler": "^20.1.1",
        "@tailwindcss/postcss": "^4.1.18",
        "autoprefixer": "^10.4.16",
        "tailwindcss": "^4.1.18",
        "typescript": "^5.3.2",
        "typescript-eslint": "^8.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "prettier": {
        "semi": true,
        "trailingComma": "all",
        "printWidth": 120,
        "tabWidth": 4,
        "singleQuote": true,
        "endOfLine": "lf"
    }
}

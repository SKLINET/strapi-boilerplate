{
    "description": "SKLINET Next.js Strapi boilerplate",
    "repository": {
        "type": "git",
        "url": "https://github.com/SKLINET/strapi-boilerplate"
    },
    "license": "MIT",
    "scripts": {
        "build": "env-cmd --silent graphql-codegen --config codegen.yml && npm run relay && NODE_ENV=production next build",
        "build-storybook": "build-storybook -c .storybook -o ./public/storybook --quiet",
        "dev": "next dev",
        "generate-models": "node ./scripts/generateModels.js",
        "export": "NODE_ENV=production next export",
        "create-blocks": " node ./scripts/createBlocks.js && npm run relay",
        "graphql-codegen": "env-cmd -f .env graphql-codegen --config codegen.yml",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "relay": "relay-compiler",
        "start": "NODE_ENV=production next start",
        "storybook": "start-storybook --no-dll",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {
        "@hookform/resolvers": "^2.9.5",
        "@react-google-maps/api": "^2.7.0",
        "@symbio/cms": "^1.0.0-canary.60",
        "@symbio/headless": "canary",
        "axios": "^0.26.0",
        "busboy": "^1.6.0",
        "clsx": "^1.2.1",
        "dayjs": "^1.11.5",
        "dotenv-webpack": "^8.0.1",
        "graphql": "^15.7.2",
        "graphql-tag": "^2.12.6",
        "hls.js": "^1.1.5",
        "html-react-parser": "^3.0.4",
        "http-proxy": "^1.18.1",
        "iron-session": "^6.3.1",
        "mandrill-api": "^1.0.45",
        "next": "^13.0.4",
        "nextjs-progressbar": "0.0.16",
        "nodemailer": "^6.7.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.37.0",
        "react-18-image-lightbox": "^5.1.4",
        "react-relay": "^14.1.0",
        "react-responsive-carousel": "^3.2.23",
        "relay-runtime": "^14.1.0",
        "storybook-addon-swc": "^1.1.9",
        "swr": "^1.3.0",
        "uuid": "^8.3.2",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^2.6.2",
        "@graphql-codegen/introspection": "2.1.1",
        "@graphql-codegen/schema-ast": "^2.4.1",
        "@next/bundle-analyzer": "^12.1.0",
        "@storybook/addon-a11y": "^6.5.12",
        "@storybook/addon-actions": "^6.5.12",
        "@storybook/addon-backgrounds": "^6.5.12",
        "@storybook/addon-console": "^1.2.3",
        "@storybook/addon-controls": "^6.5.12",
        "@storybook/addon-docs": "^6.5.12",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/addon-storysource": "^6.5.12",
        "@storybook/addon-viewport": "^6.5.12",
        "@storybook/addons": "^6.5.12",
        "@storybook/builder-webpack5": "^6.5.12",
        "@storybook/client-api": "^6.5.12",
        "@storybook/manager-webpack5": "^6.5.12",
        "@storybook/react": "^6.5.12",
        "@svgr/webpack": "^6.4.0",
        "@tailwindcss/line-clamp": "^0.3.1",
        "@types/busboy": "^1.5.0",
        "@types/http-proxy": "^1.17.9",
        "@types/jest": "^27.4.1",
        "@types/mandrill-api": "^1.0.30",
        "@types/node": "^16.11.26",
        "@types/nodemailer": "^6.4.4",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.13",
        "@types/react-relay": "^14.1.2",
        "@types/relay-runtime": "^14.1.4",
        "@types/uuid": "^8.3.4",
        "@types/yup": "^0.29.13",
        "@typescript-eslint/eslint-plugin": "^5.13.0",
        "@typescript-eslint/parser": "^5.13.0",
        "@welldone-software/why-did-you-render": "^7.0.1",
        "autoprefixer": "^10.4.2",
        "dotenv": "^16.0.3",
        "env-cmd": "^10.1.0",
        "eslint": "^8.10.0",
        "eslint-config-next": "^13.0.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.3",
        "jest": "^27.5.1",
        "lint-staged": "^12.3.5",
        "postcss": "^8.4.8",
        "prettier": "^2.5.1",
        "relay-compiler": "^12.0.0",
        "relay-compiler-language-typescript": "^15.0.1",
        "relay-config": "^12.0.1",
        "sass": "^1.49.9",
        "sass-loader": "^12.6.0",
        "tailwindcss": "^3.0.23",
        "ts-jest": "^27.1.3",
        "typescript": "^4.6.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "prettier": {
        "semi": true,
        "trailingComma": "all",
        "printWidth": 120,
        "tabWidth": 4,
        "singleQuote": true
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "es2021": true,
            "node": true
        },
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "ecmaFeatures": {
                "jsx": true
            },
            "ecmaVersion": 12,
            "sourceType": "module"
        },
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended",
            "next",
            "next/core-web-vitals",
            "plugin:prettier/recommended"
        ]
    }
}
